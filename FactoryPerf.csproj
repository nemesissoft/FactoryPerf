<Project Sdk="Microsoft.NET.Sdk">

	<PropertyGroup>
		<OutputType>Exe</OutputType>
		<TargetFramework>net5.0</TargetFramework>

		<!--<CopyLocalLockFileAssemblies>true</CopyLocalLockFileAssemblies>-->

		<!--Be able to browse generated files-->
		<EmitCompilerGeneratedFiles>true</EmitCompilerGeneratedFiles>
		<CompilerGeneratedFilesOutputPath>$(BaseIntermediateOutputPath)Generated</CompilerGeneratedFilesOutputPath>
	</PropertyGroup>




	<PropertyGroup>
		<DebugSourceGenerators>false</DebugSourceGenerators>
		<DebugAutoFactoryGenerator>true</DebugAutoFactoryGenerator>
	</PropertyGroup>

	<ItemGroup>
		<CompilerVisibleProperty Include="DebugSourceGenerators" />
		<CompilerVisibleProperty Include="DebugAutoFactoryGenerator" />
	</ItemGroup>

	<ItemGroup>
		<ProjectReference Include="..\AutoFactory\AutoFactory.csproj" OutputItemType="Analyzer" ReferenceOutputAssembly="false" />
	</ItemGroup>

</Project>
